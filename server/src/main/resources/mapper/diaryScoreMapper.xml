<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.feelog.mapper.DiaryScoreMapper">

    <select id="findById" resultType="DiaryScoreVO">
        SELECT id, score_message, score_file_path, score_file_name
        FROM tbl_diary_score
        WHERE id = #{id}
    </select>

</mapper>